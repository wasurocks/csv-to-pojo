      "IsoRequest": {
        "type": "object",
        "properties": {
          "amount": {
            "type": "number",
            "description": "Transaction amount in THB . Must be greater than 0.",
            "format": "double"
          },
          "endToEndId": {
            "type": "string",
            "description": "Unique end-to-end transaction identifier. Must be UUID format.",
            "maxLength": 36
          },
          "categoryPurpose": {
            "type": "string",
            "description": "Transaction category purpose code (e.g., ' RFT '). Must be a valid BOT category.",
            "maxLength": 4
          },
          "referenceNo": {
            "type": "string",
            "description": "Unique reference number assigned for the transaction. Copy as text Bahtnet Reference Number Mandatory for CMS, ALS(SM), PMH Pattern : BBBBTSSSSS BBBB: Branch No(0700) T : From Application 0 = CMS 7 = ALS-SM(PN) 8 = PMH SSSSS : Sequence No",
            "maxLength": 10
          },
          "messageType": {
            "type": "string",
            "description": "Transaction message type. Valid values: '008'",
            "maxLength": 3
          },
          "date": {
            "type": "string",
            "description": "Transaction date in YYYY-MM-DD format. Must be a valid date.",
            "format": "date"
          },
          "instructionForCdtrAgent": {
            "type": "string",
            "description": "Description (วัตถุประสงค์การโอนเงิน)",
            "maxLength": 140
          },
          "cdtr": {
            "$ref": "#/components/schemas/IsoRequestCdtr"
          },
          "dbtr": {
            "$ref": "#/components/schemas/IsoRequestDbtr"
          }
        },
        "required": [
          "amount",
          "categoryPurpose",
          "referenceNo",
          "messageType",
          "date",
          "cdtr",
          "dbtr"
        ]
      },
